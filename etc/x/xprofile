#!/bin/sh
#autostart file

#dbus
dbus-update-activation-environment --systemd DBUS_SESSION_BUS_ADDRESS DISPLAY XAUTHORITY

#repeat rate
xset r rate 250 50

#wallpaper
hsetroot -solid "#d3cdcb"

#important things
picom -b --config ~/etc/picom/picom.conf &
sxhkd -c ~/etc/sxhkd/sxhkdrc &

#rm daemon
ch &

#choose your WM
exec openbox
#exec glazier
