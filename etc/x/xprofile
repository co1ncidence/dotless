#!/bin/sh
# X11 Startup File

# Start dbus
dbus-update-activation-environment --systemd DBUS_SESSION_BUS_ADDRESS DISPLAY XAUTHORITY

# Set repeat rate
xset r rate 250 50

# Set wallpaper
hsetroot -cover "$HOME/med/wlp/coffifull.jpg"

# Launch some programs
picom -b --config ~/etc/picom/picom.conf &
sxhkd -c ~/etc/sxhkd/sxhkdrc &

# Start RM Daemon
ch &

# Start a wm
exec openbox
