#!/bin/sh
#dmemenu

dir="$HOME/med/pic/mem"

mem="$(ls -1 "$dir" | dmenu -fn "Iosevka Term Slab" \
                                      -nb "#515151" \
                                      -nf "#ffffff" \
                                      -sb "#515151" \
                                      -sf "#66948b")"

[ -f "$dir/$mem" ] && xclip -sel clip -t image/png "$dir/$mem"
