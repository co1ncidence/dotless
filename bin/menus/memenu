#!/bin/sh
#dmemenu

dir="$HOME/med/pic/mem"

mem="$(ls -1 "$dir" \
  | dmenu -fn "Iosevka Term Slab" \
                    -nb "#3e393b" \
                    -nf "#7b7274" \
                    -sb "#3e393b" \
                    -sf "#e4e1e2")"

[ -f "$dir/$mem" ] && xclip -sel clip -t image/png "$dir/$mem"
